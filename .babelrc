{
  "presets": [
      [ "@babel/preset-env",
          {
              "modules": false,
              "useBuiltIns": "usage"
          }
      ],
      "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
  ],
  "env": {
      "test": {
